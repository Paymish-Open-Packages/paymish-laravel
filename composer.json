{
    "name": "paymish/paymish-laravel",
    "description": "Laravel package for Paymish payment gateway.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Paymish\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paymish\\PaymishServiceProvider"
            ],
            "aliases": {
                "Paymish": "Paymish\\Facades\\Paymish"
            }
        }
    }
}
